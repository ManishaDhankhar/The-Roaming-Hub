 <% if (success && success.length > 0) { %>
    <div class="alert alert-success alert-dismissible col-4 offset-4" role="alert" >
        <%= success %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<% }%>
<% if(error && error.length>0){%>
    <div class="alert alert-error alert-dismissible col-4 offset-4" role="alert">
        <%= error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
  <% } %>
